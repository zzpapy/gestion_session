{% extends 'base.html.twig' %}
{% block body %}
{{ form_start(form) }}
<div class="container flex_form">
    <div>
        {{ form_row(form.nom) }} 
        {{ form_row(form.prenom) }}
        {{ form_row(form.sexe) }} 
        {{ form_row(form.birth)}}   
        {{ form_row(form.ville) }}
        <div id="hits"></div> 
        {{ form_row(form.email) }} 
        {{ form_row(form.phone) }} 
        {{ form_row(form.photo,{ 'attr': {'onchange': 'readURL(this);'} }) }} 
    </div>
    <div class="prev">
        {% if stagiaire.photo !="" %}
        <img id="blah" src="{{asset('img/'~stagiaire.photo)}}" alt="">
        {% else %}
        <img id="blah" src="{{asset('img/avatar.jpg')}}" alt="">            
        {% endif %}
    </div>
</div>
    {{ form_row(form.submit) }}
{{ form_end(form) }}
{% endblock %}

